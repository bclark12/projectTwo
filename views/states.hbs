<h1>All States</h1>
<nav>
    <a href="/main">Home</a>
    <a href="/states">View All States</a>
    <a href="/schools">View All Schools</a>
    <a href="/teams">View All Teams</a>
</nav>
<nav>
    <a href="/schools/chooseStateForSchools">See Schools By State</a>
    <a href="/teams/chooseStateForTeams">Choose Team By state</a>
</nav>
<div class="container">
<table class="table">
  <tr>
    <th></th>
    <th id="stateHeader">State</th>
    <th id="capitalHeader">Capital</th> 
    <th>Population</th>
    <th>Higher Education Rank</th>
    <th>Schools</th>
  </tr>
  {{#each statesInDB}}
  <tr>
    <td><a href="/states/{{_id}}">Edit {{state}}</a></td>
    <td>{{state}}</td>
    <td>{{capital}}</td>
    <td>{{population}}</td>
    <td id="educationRank">{{higherEducationRank}}</td>
    <td>{{schools}}</td>
    <td><a href="/schools/{{_id}}">See {{state}} Colleges</a></td>
    
    
  </tr>
  {{/each}}
</table>
</div>
<ul>
<a href="/states/addState">Add New State</a>
</ul>
<form method="POST" action="/states/?_method=DELETE">
    <input type="submit" value="Delete Noname States"/>
</form>


